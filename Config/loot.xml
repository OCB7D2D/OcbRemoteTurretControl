<configs patcher-version="1">

	<modif condition="UndeadLegacy_CoreModule">
		<append xpath="/lootcontainers/lootgroup[@name='booksElectrical']">
			<item name="ocbRemoteTurretPanelBlueprint" loot_prob_template="med"/>
		</append>
	</modif>
	<modelse>
		<append xpath="/lootcontainers/lootgroup[@name='schematicsElectrical']">
			<item name="ocbRemoteTurretPanelSchematic" loot_prob_template="medLow"/>
		</append>
		<append xpath="/lootcontainers/lootgroup[@name='schematicsToolsT2']">
			<item name="ocbRemoteTurretPanelSchematic" loot_prob_template="low"/>
		</append>
	</modelse>

</configs>